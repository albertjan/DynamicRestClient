    <version>0.0.0.3</version>
    <title>A Dynamic REST Client</title>
    <authors>Albert-Jan Nijburg</authors>
    <owners>Albert-Jan Nijburg</owners>
    <licenseUrl>https://github.com/albertjan/DynamicRestClient/blob/master/license.txt</licenseUrl>
